    "firebase-admin": "^8.0.0",
    "graphql": "^14.3.1",
    "lodash": "^4.17.19"
  },
  "devDependencies": {
    "dotenv": "^8.0.0",
    "flow-bin": "^0.100.0",
    "rimraf": "^2.6.3",
    "rollup": "^1.13.1",
    "rollup-plugin-flow": "^1.1.1",
