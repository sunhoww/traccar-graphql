{
  "name": "traccar-graphql",
  "version": "0.0.0",
  "description": "GraphQL server for traccar",
  "main": "src/index.js",
  "scripts": {
    "start": "npm run prod",
    "serve:dev": "NODE_ENV=development node ./dist/index.js",
    "serve:prod": "NODE_ENV=production node ./dist/index.js",
    "dev": "npm run server:dev",
    "clean": "rimraf dist",
    "build": "rollup -c",
    "prod": "NODE_ENV=production npm run clean && npm run build && npm run server:prod",
    "watch": "rollup -c --watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Sun Howwrongbum <sun@libermatic.com>",
  "license": "MIT",
  "dependencies": {
    "apollo-datasource-rest": "^0.3.2",
    "apollo-server": "^2.4.8",
    "dotenv": "^8.0.0",
    "graphql": "^14.2.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "rimraf": "^2.6.3",
    "rollup": "^1.11.3",
    "rollup-plugin-run": "^1.1.0"
  }
}
